{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debugging action.py",
      "type": "python",
      "request": "launch",
      "program": "action.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/aws_lib",
      "args": ["scan-table"],
      "windows": {
        "cwd": "${workspaceFolder}\\aws_lib"
      }
    },
    {
      "name": "pytest single method",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["-k", "test_list_executions"],
      "justMyCode": true
    }
  ]
}
